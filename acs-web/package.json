{
  "name": "@bonzi/acs-web",
  "version": "0.1.0",
  "description": "WASM bindings for parsing and rendering Microsoft Agent (.acs) files",
  "type": "module",
  "main": "pkg/acs_web.js",
  "types": "pkg/acs_web.d.ts",
  "files": [
    "pkg"
  ],
  "scripts": {
    "build": "wasm-pack build --target web --out-dir pkg",
    "build:bundler": "wasm-pack build --target bundler --out-dir pkg",
    "build:nodejs": "wasm-pack build --target nodejs --out-dir pkg"
  },
  "keywords": [
    "wasm",
    "webassembly",
    "microsoft-agent",
    "acs",
    "bonzi"
  ],
  "license": "MIT"
}
